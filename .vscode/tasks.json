{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "g++-9 build active file",
      "command": "/usr/bin/g++-9",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${workspaceFolder}/exe/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "/usr/bin"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
